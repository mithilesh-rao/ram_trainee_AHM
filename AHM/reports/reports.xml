<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <report
        id="report_stock"
        model="ahm.stock"
        string="Stock Report"
        name="AHM.report_stock_view"
        file="AHM.report_stock"
        report_type="qweb-pdf" />

    <template id="report_stock_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                         <p> <h1>Name</h1> <span t-field="docs.name"/></p>
                         <p> <h1>Company</h1> <span t-field="docs.comp"/></p>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>